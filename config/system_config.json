{
  "telemetry": {
    "update_interval_seconds": 3,
    "processing_timeout_ms": 250,
    "validation_enabled": true,
    "fallback_to_last_valid": true,
    "output_file": "shared/telemetry_state.json",
    "use_simulator": true,
    "live_source_type": "websocket",
    "max_failures_before_fallback": 5,
    "live_source": {
      "websocket_url": "ws://localhost:8080/telemetry",
      "host": "localhost",
      "port": 20777,
      "timeout": 5.0,
      "max_reconnect_attempts": 5,
      "reconnect_delay": 2.0
    }
  },
  "car_twin": {
    "update_latency_ms": 200,
    "prediction_horizon_laps": 10,
    "tire_degradation_model": "linear",
    "fuel_consumption_model": "track_specific"
  },
  "field_twin": {
    "update_latency_ms": 300,
    "competitor_count": 19,
    "behavioral_analysis_enabled": true,
    "resimulation_triggers": ["pit_stop", "safety_car", "position_change"]
  },
  "state_management": {
    "persistence_interval_seconds": 5,
    "backup_enabled": true,
    "consistency_check_enabled": true,
    "audit_logging_enabled": true,
    "storage_path": "shared"
  },
  "api": {
    "host": "localhost",
    "port": 8000,
    "response_timeout_ms": 50,
    "max_concurrent_connections": 10,
    "enable_cors": true,
    "api_version": "v1"
  },
  "performance": {
    "max_memory_mb": 1024,
    "gc_interval_seconds": 60,
    "profiling_enabled": false,
    "metrics_collection_enabled": true
  },
  "orchestrator": {
    "loop_interval_seconds": 1.0,
    "telemetry_check_interval_seconds": 3.0,
    "state_persistence_interval_seconds": 5.0,
    "performance_monitoring_enabled": true,
    "auto_optimization_enabled": true
  },
  "monitoring": {
    "enabled": true,
    "interval_seconds": 10,
    "alert_retention_hours": 24,
    "auto_optimization_enabled": true,
    "health_check_enabled": true,
    "performance_thresholds": {
      "telemetry_processing_time_ms": 250,
      "car_twin_update_time_ms": 200,
      "field_twin_update_time_ms": 300,
      "api_response_time_ms": 50,
      "cpu_usage_percent": 85,
      "memory_usage_percent": 90,
      "disk_usage_percent": 95
    }
  },
  "recovery": {
    "enabled": true,
    "startup_recovery_enabled": true,
    "auto_recovery_on_inconsistency": true,
    "checkpoint_interval_seconds": 30,
    "max_recovery_attempts": 3
  },
  "car": {
    "our_car_id": "44"
  },
  "race": {
    "total_laps": 70
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "file_path": "shared/logs/f1_twin.log",
    "max_file_size_mb": 100,
    "backup_count": 5,
    "file": "f1_twin_system.log"
  }
}